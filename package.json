{
  "name": "RN-test",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^8.2.6",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.0.0",
    "jest-expo": "~27.0.0",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.3.1",
    "reactotron-react-native": "^2.1.0",
    "reactotron-redux": "^2.1.0",
    "reactotron-redux-saga": "^2.1.0"
  },
  "main": "./index.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/App/**/tests/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/App/Testing/enzyme-adapter.js"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "expo": "^27.0.1",
    "immutable": "^3.8.2",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "reselect": "^3.0.1",
    "styled-components": "^3.4.9",
    "whatwg-fetch": "^2.0.4"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
